ton\n  &-cart {\n    padding: 1.5rem 3rem;\n    & i {\n      font-size: 2.6rem;\n      margin: 0;\n    }\n  }\n}\n\n/////////////\n// Sizes Selector for Product\n.sizes {\n  display: inline-block;\n}\n\n.size-btn {\n  display: inline-block;\n  border: none;\n  outline: none;\n  text-align: center;\n  color: $color-light-1;\n  background: rgba(#ffffff, 0.2);\n  @include fontSm;\n  padding: 1rem 2rem;\n  margin-right: 0.5rem;\n  cursor: pointer;\n  :last-child {\n    margin-right: none;\n  }\n}\n\n.size-active {\n  border: 1px solid white;\n  color: white;\n  background: rgba(#ffffff, 0.7);\n}\n\n/////////////\n// Colors Selector for Product\n.colors {\n  width: 25rem;\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n  flex-wrap: wrap;\n}\n\n.color {\n  margin-right: 1.25rem;\n  margin-bottom: 2rem;\n  display: inline-block;\n  height: 4rem;\n  width: 4rem;\n  border-radius: 50%;\n  transition: all 0.3s ease-in-out;\n  cursor: pointer;\n  background-color: $color-light-2;\n  border: 0.5px solid #adadad;\n}\n\n.color-active {\n  border: 4px solid white;\n}\n\n// Cart Amount\n.cart__amount-num {\n  position: absolute;\n  top: -10px;\n  right: -15px;\n  background-color: $color-dark-1;\n  color: #ffffff;\n  border-radius: 100px;\n  @include fontSm;\n  display: block;\n  text-align: center;\n  width: 3rem;\n  height: 3rem;\n}\n","// Variables\n// Fonts\n// Mixins\n// user: <i class=\"fas fa-user\"></i>\n// cart: <i class=\"fas fa-shopping-bag\"></i>\n// search: <i class=\"fas fa-search\"></i>\n// menu: <i class=\"fas fa-bars\"></i>\n// ViewProduct: <i class=\"fas fa-plus-circle\"></i>\n\n$color-light-1: #c8cfc8;\n$color-light-2: #d79e58;\n$color-light-3: #e9ab3c;\n$color-dark-1: #357b72;\n$color-dark-2: #784c37;\n\n@mixin fontXl {\n  font-size: 9.6rem;\n  font-weight: 500;\n  letter-spacing: 2px;\n}\n\n@mixin fontLg {\n  font-size: 5.5rem;\n  font-weight: 500;\n  letter-spacing: 2px;\n}\n\n@mixin fontMd {\n  font-size: 3.6rem;\n  font-weight: 500;\n  letter-spacing: 1.5px;\n}\n\n@mixin fontSm {\n  font-size: 2.4rem;\n  font-weight: 500;\n  letter-spacing: 1.5px;\n}\n\n@mixin fontXs {\n  font-size: 1.8rem;\n  letter-spacing: 1.5px;\n}\n\n@mixin drawUnderline {\n  &:after {\n    content: \"\";\n    height: 3px;\n    width: 0;\n    background: $color-light-2;\n    position: absolute;\n    bottom: 0;\n    left: 0;\n  }\n\n  &:hover:after {\n    animation: underline 0.5s ease-in;\n    animation-fill-mode: forwards;\n  }\n}\n","// Navigation(side)\n.nav-container {\n  display: flex;\n}\n\n.nav-container-side {\n  position: absolute;\n  top: 0;\n  right: -100%;\n  z-index: 101;\n  display: none;\n  background-color: rgba(0, 0, 0, 0.1);\n  width: 100%;\n  height: 100%;\n  transition: all 0.5s ease-in;\n  //   transition-delay: .5s;\n}\n.nav-container-active {\n  right: 0;\n  display: block;\n}\n\n.nav {\n  position: absolute;\n  top: 0;\n  left: -25%;\n  text-align: center;\n  color: #fff;\n  height: 100%;\n  width: 25%;\n  background-color: #000;\n  display: none;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  z-index: 200;\n  transition: all 2s ease-in;\n\n  &__header {\n    margin: 5rem 0;\n    @include fontXl;\n    text-transform: uppercase;\n  }\n\n  &__list {\n    height: 30%;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n\n    &-item {\n      @include fontMd;\n\n      a {\n        color: #fff;\n        position: relative;\n        @include drawUnderline;\n      }\n    }\n  }\n\n  .social-icons {\n    margin-top: auto;\n    margin-bottom: 5rem;\n    width: 20rem;\n    display: flex;\n    justify-content: space-around;\n\n    & a {\n      cursor: pointer;\n    }\n  }\n}\n\n.nav-active {\n  left: 0;\n  display: flex;\n}\n",".header {\n  width: 100vw;\n  height: 10rem;\n  margin-top: 2rem;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: absolute;\n  z-index: 100;\n\n  a.header__nav-btn {\n    c