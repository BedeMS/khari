ify-content: space-between;\n  align-items: center;\n  z-index: 200;\n  transition: all 2s ease-in;\n}\n.nav__header {\n  margin: 5rem 0;\n  font-size: 9.6rem;\n  font-weight: 500;\n  letter-spacing: 2px;\n  text-transform: uppercase;\n}\n.nav__list {\n  height: 30%;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n}\n.nav__list-item {\n  font-size: 3.6rem;\n  font-weight: 500;\n  letter-spacing: 1.5px;\n}\n.nav__list-item a {\n  color: #fff;\n  position: relative;\n}\n.nav__list-item a:after {\n  content: \"\";\n  height: 3px;\n  width: 0;\n  background: #d79e58;\n  position: absolute;\n  bottom: 0;\n  left: 0;\n}\n.nav__list-item a:hover:after {\n  animation: underline 0.5s ease-in;\n  animation-fill-mode: forwards;\n}\n.nav .social-icons {\n  margin-top: auto;\n  margin-bottom: 5rem;\n  width: 20rem;\n  display: flex;\n  justify-content: space-around;\n}\n.nav .social-icons a {\n  cursor: pointer;\n}\n.nav-active {\n  left: 0;\n  display: flex;\n}\n.header {\n  width: 100vw;\n  height: 10rem;\n  margin-top: 2rem;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: absolute;\n  z-index: 100;\n}\n.header a.header__nav-btn {\n  color: #fff;\n  margin-left: 5rem;\n  margin-right: auto;\n  cursor: pointer;\n  z-index: 250;\n}\n.header__menu {\n  font-size: 3.6rem;\n  font-weight: 500;\n  letter-spacing: 1.5px;\n}\n.header__menu-item {\n  color: #fff;\n  text-transform: uppercase;\n  letter-spacing: 1rem;\n}\n.header__menu a.header__menu-item {\n  text-decoration: underline;\n}\n.header__menu a.header__menu-item img {\n  height: 15rem;\n  position: absolute;\n  top: 10%;\n}\n.header__icons {\n  width: 20rem;\n  height: auto;\n  display: flex;\n  justify-content: space-around;\n  margin-left: auto;\n  margin-right: 5rem;\n}\n.header__icons i {\n  color: #fff;\n  cursor: pointer;\n}\ni.fas-dark,\ni.fab-dark {\n  color: #000000;\n}\ni.fas-grey {\n  color: grey;\n}\n.header__menu.header__dark a,\n.header__menu.header__dark p {\n  color: #000000;\n}\n.header__short {\n  width: 60vw;\n  display: flex;\n  justify-content: flex-start;\n  align-items: flex-end;\n}\n.header__short a.header__nav-btn {\n  margin-right: 2rem;\n}\n.header__short .header__menu {\n  font-size: 1.8rem;\n  letter-spacing: 1.5px;\n}\n.header__short .header__menu-img img {\n  height: 15rem;\n  position: absolute;\n  top: 10%;\n  right: 5%;\n}\n.header__long {\n  width: 100vw;\n  display: flex;\n  justify-content: flex-start;\n  align-items: flex-end;\n}\n.header__long a.header__nav-btn {\n  margin-right: 1rem;\n}\n.header__long .header__menu {\n  font-size: 1.8rem;\n  letter-spacing: 1.5px;\n}\n.header__long .header__menu p {\n  display: inline-block;\n}\n.header__long .header__menu-img img {\n  height: 15rem;\n  position: absolute;\n  top: 10%;\n  right: 50%;\n}\n.menu__active {\n  font-size: 3.6rem;\n  font-weight: 500;\n  letter-spacing: 1.5px;\n}\n.main {\n  height: 100%;\n  width: 100%;\n  background: #000000;\n  text-align: center;\n  position: relative;\n}\n.main__heading {\n  position: absolute;\n  top: 50%;\n  transform: translateY(-50%);\n  margin-left: 8rem;\n}\n.main__heading-desc {\n  font-size: 2.4rem;\n  font-weight: 500;\n  letter-spacing: 1.5px;\n  letter-spacing: 1rem;\n  margin-bottom: 5rem;\n}\n.main__heading-title {\n  font-size: 20rem;\n  text-transform: uppercase;\n  letter-spacing: 1.5rem;\n}\n.main__heading p {\n  color: white;\n}\n.main__half {\n  position: relative;\n  display: block;\n  background: #357b72;\n  width: auto;\n  height: 100%;\n  clip-path: polygon(44% 0, 41% 3%, 40% 5%, 40% 7%, 40% 9%, 41% 12%, 41% 13%, 42% 15%, 43% 16%, 44% 18%, 45% 20%, 46% 22%, 47% 23%, 49% 26%, 50% 27%, 52% 29%, 53% 31%, 54% 32%, 55% 34%, 56% 37%, 57% 39%, 57% 42%, 57% 45%, 57% 48%, 57% 50%, 56% 53%, 56% 55%, 55% 58%, 54% 60%, 53% 63%, 51% 66%, 49% 69%, 47% 70%, 45% 72%, 42% 74%, 40% 76%, 38% 78%, 35% 80%, 32% 82%, 29% 83%, 27% 85%, 23% 87%, 20% 88%, 17% 89%, 15% 90%, 14% 90%, 12% 91%, 10% 91